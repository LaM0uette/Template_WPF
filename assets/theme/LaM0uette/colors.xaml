<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="M1">#FF3A3A41</Color>
    <Color x:Key="M2">#FF8F8FA3</Color>
    <Color x:Key="M3">#FFFFFAFF</Color>
    <Color x:Key="B1">#FF44AAB8</Color>
    <Color x:Key="B2">#FFE53843</Color>
    <Color x:Key="Ok1">#A1E510</Color>
    <Color x:Key="Ok2">#2DC8AB</Color>
    <Color x:Key="Nok1">#F11726</Color>
    <Color x:Key="Nok2">#FFDC562E</Color>
    
    <SolidColorBrush x:Key="RgbM1" Color="{StaticResource M1}"/>
    <SolidColorBrush x:Key="RgbM2" Color="{StaticResource M2}"/>
    <SolidColorBrush x:Key="RgbM3" Color="{StaticResource M3}"/>
    <SolidColorBrush x:Key="RgbB1" Color="{StaticResource B1}"/>
    <SolidColorBrush x:Key="RgbB2" Color="{StaticResource B2}"/>
    <SolidColorBrush x:Key="RgbOk1" Color="{StaticResource Ok1}"/>
    <SolidColorBrush x:Key="RgbOk2" Color="{StaticResource Ok2}"/>
    <SolidColorBrush x:Key="RgbNok1" Color="{StaticResource Nok1}"/>
    <SolidColorBrush x:Key="RgbNok2" Color="{StaticResource Nok2}"/>

</ResourceDictionary>
